<div class="row">
    <div class="col-md-12">
      <% flash.get('mensaje').forEach(function (msg) {%>
      <div class="alert alert-info text-center" role="alert"><%=msg%></div>
      <%})%>
  
      <h2>Ordenes</h2>
  
      <table class="table">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Fecha</th>
            <th scope="col">Total Fotos</th>
            <th scope="col">Detalle</th>
          </tr>
        </thead>
        <tbody>
          <%ordenes.forEach( orden => {%>
          <tr>
            <td scope="row"><%=orden.id%></td>
            <td scope="row"><%=orden.fecha%></td>
            <td scope="row"><%=orden.total%></td>
            <td scope="row"><a class="nav-link" href="/admin/detalle/<%=orden.id%>"><i class="fas fa-receipt"></i></a></td>
          </tr>
          <%})%>
        </tbody>
      </table>
    </div>
  </div>
  